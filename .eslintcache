omponent={Home}></Route>\n        <PrivateRoute path=\"/chat\" authenticated={authed} component={Chat} />\n        <PublicRoute path=\"/signup\" authenticated={authed} component={Signup} />\n        <PublicRoute path=\"/login\" authenticated={authed} component={Login} />\n      </Switch>\n    </Router>\n  );\n}\n\nexport default App;\n",["48","49"],"/Users/anduser/Documents/GB/reactjs/firebaseexample/src/reportWebVitals.js",[],"/Users/anduser/Documents/GB/reactjs/firebaseexample/src/hocs/PrivateRoute.js",[],"/Users/anduser/Documents/GB/reactjs/firebaseexample/src/services/firebase.js",[],"/Users/anduser/Documents/GB/reactjs/firebaseexample/src/hocs/PublicRoute.js",[],"/Users/anduser/Documents/GB/reactjs/firebaseexample/src/components/Login/index.js",["50","51"],"import React, { Component } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { signin, signInWithGoogle, signInWithGitHub } from \"../../services/firebase\";\n\nexport default class Login extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      error: null,\n      email: \"\",\n      password: \"\",\n    };\n    this.handleChange = this.handleChange.bind(this);\n    this.handleSubmit = this.handleSubmit.bind(this);\n  }\n\n  handleChange(event) {\n    this.setState({\n      [event.target.name]: event.target.value,\n    });\n  }\n\n  async handleSubmit(event) {\n    event.preventDefault();\n    this.setState({ error: \"\" });\n    try {\n      await signin(this.state.email, this.state.password);\n    } catch (error) {\n      this.setState({ error: error.message });\n    }\n  }\n\n  render() {\n    return (\n      <div>\n        <form autoComplete=\"off\" onSubmit={this.handleSubmit}>\n          <h1>\n            Login to\n            <Link to=\"/\">Chatty</Link>\n          </h1>\n          <p>Fill in the form below to login to your account.</p>\n          <div>\n            <input\n              placeholder=\"Email\"\n              name=\"email\"\n              type=\"email\"\n              onChange={this.handleChange}\n              value={this.state.email}\n            />\n          </div>\n          <div>\n            <input\n              placeholder=\"Password\"\n              name=\"password\"\n              onChange={this.handleChange}\n              value={this.state.password}\n              type=\"password\"\n            />\n          </div>\n          <div>\n            {this.state.error ? <p>{this.state.error}</p> : null}\n            <button type=\"submit\">Login</button>\n          </div>\n          <hr />\n          <p>\n            Don't have an account? <Link to=\"/signup\">Sign up</Link>\n          </p>\n        </form>\n      </div>\n    );\n  }\n}\n","/Users/anduser/Documents/GB/reactjs/firebaseexample/src/components/Chat/index.js",[],"/Users/anduser/Documents/GB/reactjs/firebaseexample/src/components/Home/index.js",[],"/Users/anduser/Documents/GB/reactjs/firebaseexample/src/components/SignUp/index.js",["52","53"],"import React, { useState, useCallback } from \"react\";\nimport { Link } from \"react-router-dom\";\n\nimport { signup } from \"../../services/firebase\";\n\nexport default function SignUp() {\n  const [error, setError] = useState('');\n  const [userValues, setUserValues] = useState({\n    email: \"\",\n    password: \"\",\n  });\n\n  const handleSubmit = useCallback(async (e) => {\n    e.preventDefault();\n    setError('');\n\n    try {\n      await signup(userValues.email, userValues.password);\n    } catch (err) {\n      setError(err.message);\n    }\n  }, [userValues]);\n\n  const handleChangeEmail = useCallback((e) => {\n    setUserValues(values => ({ ...values, email: e.target.value }));\n  }, [userValues]);\n\n  const handleChangePass = useCallback(\n    (e) => {\n      setUserValues((values) => ({ ...values, password: e.target.value }));\n    },\n    [userValues]\n  );\n\n  return (\n    <div>\n      <form onSubmit={handleSubmit}>\n        <h1>\n          Sign Up to\n          <Link to=\"/\">Chatty</Link>\n        </h1>\n        <p>Fill in the form below to create an account.</p>\n        <div>\n          <input\n            placeholder=\"Email\"\n            name=\"email\"\n            type=\"email\"\n            onChange={handleChangeEmail}\n            value={userValues.email}\n          ></input>\n        </div>\n        <div>\n          <input\n            placeholder=\"Password\"\n            name=\"password\"\n            onChange={handleChangePass}\n            value={userValues.password}\n            type=\"password\"\n          ></input>\n        </div>\n        <div>\n          {error && <p>{error}</p>}\n          <button type=\"submit\">Sign up</button>\n        </div>\n        <hr></hr>\n        <p>\n          Already have an account? <Link to=\"/login\">Login</Link>\n        </p>\n      </form>\n    </div>\n  );\n}\n",{"ruleId":"54","severity":1,"message":"55","line":1,"column":17,"nodeType":"56","messageId":"57","endLine":1,"endColumn":26},{"ruleId":"54","severity":1,"message":"58","line":6,"column":3,"nodeType":"56","messageId":"57","endLine":6,"endColumn":11},{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","replacedBy":"62"},{"ruleId":"54","severity":1,"message":"63","line":3,"column":18,"nodeType":"56","messageId":"57","endLine":3,"endColumn":34},{"ruleId":"54","severity":1,"message":"64","line":3,"column":36,"nodeType":"56","messageId":"57","endLine":3,"endColumn":52},{"ruleId":"65","severity":1,"message":"66","line":26,"column":6,"nodeType":"67","endLine":26,"endColumn":18,"suggestions":"68"},{"ruleId":"65","severity":1,"message":"66","line":32,"column":5,"nodeType":"67","endLine":32,"endColumn":17,"suggestions":"69"},"no-unused-vars","'Component' is defined but never used.","Identifier","unusedVar","'Redirect' is defined but never used.","no-native-reassign",["70"],"no-negated-in-lhs",["71"],"'signInWithGoogle' is defined but never used.","'signInWithGitHub' is defined but never used.","react-hooks/exhaustive-deps","Reac